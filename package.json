{
  "name": "plugdj-franz",
  "version": "1.0.0",
  "description": "Franz plugin for Plug.DJ",
  "main": "index.js",
  "author": "Chris Martinez <chris@cmtz.co>",
  "license": "MIT",
  "config": {
    "serviceURL": "http://plug.dj/{teamID}/#login",
    "serviceName": "Plug.DJ",
    "message": "Listen to music with friends",
    "popup": [
      "^https://www.facebook.com/v2.4/dialog/oauth?"
    ],
    "hasNotificationSound": true,
    "hasIndirectMessages": true,
    "customURL": false,
    "hasTeamID": true,
    "webviewOptions": {
      "disablewebsecurity": ""
    },
    "openDevTools": true
  }
}